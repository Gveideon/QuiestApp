<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\QuestEngine\QuestEngine.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="ResourceQuest.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>ResourceQuest.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="ResourceQuest.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>ResourceQuest.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>